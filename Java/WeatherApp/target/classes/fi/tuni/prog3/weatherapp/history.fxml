<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="TOP_CENTER" prefHeight="290.0" prefWidth="307.0" spacing="20.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="fi.tuni.prog3.weatherapp.HistoryController">
    <children>
      <Label text="History:">
         <font>
            <Font size="14.0" />
         </font></Label>
      <ListView fx:id="listViewHistory" prefHeight="200.0" prefWidth="200.0" />
      <Button fx:id="goBackButton" onAction="#goBack" text="Go Back" />
    </children>
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
</VBox>
